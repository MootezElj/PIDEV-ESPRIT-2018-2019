        <!-- Main js file contents all jQuery plugins activation
        ========================================================= -->
        <script src="{{ asset('front/js/main.js') }}"></script>
        <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>

    {% endblock %}

    {% block stylesheets %}
        <!-- favicon
        ============================================ -->
        <link rel="shortcut icon" type="image/x-icon" href="{{ asset('front/images/favicon.ico') }}">

        <!-- Google Fonts
        ============================================ -->
        <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700" rel="stylesheet">

        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.1/css/all.css" integrity="sha384-fnmOCqbTlWIlj8LyTjo7mOUStjsKC4pOpQbqyi7RrhN7udi9RwhKkMHpvLbHG9Sr" crossorigin="anonymous">
        <!-- All css files are included here
        ============================================ -->
        <!-- Bootstrap CSS
        ============================================ -->
        <link rel="stylesheet" href="{{ asset('front/css/bootstrap.min.css') }}" >

        <!-- This core.css file contents all plugins css file
        ============================================ -->
        <link rel="stylesheet" href="{{ asset('front/css/core.css') }}" >


        <link rel="stylesheet" href="{{ asset('front/css/shortcode/shortcodes.css') }}" >

        <!-- Color Swithcer CSS
        ============================================ -->
        <link rel="stylesheet" href="{{ asset('front/css/plugins/color-switcher.css') }}">

        <!--  Theme main style
        ============================================ -->
        <link rel="stylesheet" href="{{ asset('front/style.css') }}">

        <!-- Color CSS
        ============================================ -->
        <link rel="stylesheet" href="{{ asset('front/css/plugins/color.css') }}">

        <!-- Responsive CSS
        ============================================ -->
        <link rel="stylesheet" href="{{ asset('front/css/responsive.css') }}">

        <link rel="stylesheet" href="{{ asset('front/css/myCss/front_style.css') }}">

    {% endblock %}

</head>
